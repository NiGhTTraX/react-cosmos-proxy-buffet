{
  "name": "react-cosmos-proxy-buffet",
  "version": "0.0.1",
  "description": "Extensions bar plugin for React Cosmos",
  "keywords": [
    "react",
    "cosmos",
    "react-cosmos",
    "proxy",
    "plugin",
    "extension"
  ],
  "main": "dist/src/index",
  "types": "dist/src/index",
  "scripts": {
    "playground": "cosmos --config playground/cosmos.config.js",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc --declaration --outDir ./dist",
    "prepublishOnly": "npm run build",
    "lint": "eslint --ext ts,tsx .",
    "test": "TS_NODE_CACHE=0 mocha --opts tests/unit/mocha.opts",
    "test:coverage": "NODE_ENV=coverage nyc mocha --opts tests/unit/mocha.coverage.opts",
    "report-coverage": "nyc report --reporter=json && codecov -f tests/results/coverage/coverage-final.json",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.tsx?": "eslint"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "dependencies": {
    "classnames": "latest",
    "reset.css": "~2.0.2",
    "store": "~2.0.12"
  },
  "peerDependencies": {
    "react": "^16",
    "react-cosmos": "^4"
  },
  "devDependencies": {
    "@babel/core": "~7.0.0",
    "@babel/plugin-proposal-class-properties": "~7.0.0",
    "@babel/preset-env": "~7.0.0",
    "@babel/preset-react": "~7.0.0",
    "@babel/preset-typescript": "~7.0.0",
    "@babel/register": "~7.0.0",
    "@nighttrax/eslint-config": "~1.0.1",
    "@types/chai": "~4.1.4",
    "@types/classnames": "~2.2.6",
    "@types/jquery": "~3.3.6",
    "@types/jsdom": "~11.0.6",
    "@types/mocha": "~5.2.5",
    "@types/node": "~10.9.4",
    "@types/react": "~16.4.13",
    "@types/react-dom": "~16.0.7",
    "@types/sinon": "~5.0.2",
    "@types/store": "~2.0.0",
    "babel-loader": "~8.0.2",
    "babel-plugin-istanbul": "~5.0.1",
    "babel-plugin-module-resolver": "~3.1.1",
    "chai": "~4.1.2",
    "codecov": "~3.1.0",
    "css-loader": "~1.0.0",
    "eslint": "~5.5.0",
    "eslint-import-resolver-babel-module": "~4.0.0",
    "eslint-plugin-import": "~2.14.0",
    "eslint-plugin-jsx-a11y": "~6.1.1",
    "eslint-plugin-react": "~7.11.1",
    "eslint-plugin-typescript": "~0.12.0",
    "html-webpack-plugin": "~3.2.0",
    "husky": "~0.14.3",
    "ignore-styles": "~5.0.1",
    "jquery": "~3.3.1",
    "jsdom": "~12.0.0",
    "less": "~3.8.1",
    "less-loader": "~4.1.0",
    "lint-staged": "~7.2.2",
    "mocha": "~5.2.0",
    "nyc": "~13.0.1",
    "react": "~16.5.0",
    "react-cosmos": "~4.6.4",
    "react-dom": "~16.5.0",
    "react-mock-component": "~0.4.1",
    "sinon": "~6.2.0",
    "style-loader": "~0.23.0",
    "ts-node": "~7.0.1",
    "typemoq": "~2.1.0",
    "typescript": "~3.0.3",
    "typescript-eslint-parser": "~18.0.0",
    "webpack": "~4.17.2"
  },
  "files": [
    "dist/src/**/*"
  ]
}
