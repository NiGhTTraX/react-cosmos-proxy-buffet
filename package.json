{
  "name": "react-cosmos-proxy-buffet",
  "version": "0.0.1",
  "description": "Extensions bar plugin for React Cosmos",
  "repository": {
    "type": "git",
    "url": "https://github.com/NiGhTTraX/react-cosmos-proxy-buffet.git"
  },
  "keywords": [
    "react",
    "cosmos",
    "react-cosmos",
    "proxy",
    "plugin",
    "extension"
  ],
  "main": "dist/src/index",
  "types": "dist/src/index",
  "scripts": {
    "start": "ACCEPTANCE=1 cosmos --config playground/cosmos.config.js",
    "playground": "cosmos --config playground/cosmos.config.js",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc --declaration --outDir ./dist",
    "prepublishOnly": "npm run build",
    "lint": "eslint --ext ts,tsx .",
    "test": "./tests/run.sh",
    "test:node": "TS_NODE_CACHE=0 mocha --opts tests/unit/mocha.opts",
    "test:node:coverage": "COVERAGE=1 nyc mocha --opts tests/unit/mocha.coverage.opts",
    "test:gui": "./tests/gui/run.sh",
    "test:gui:debug": "./tests/gui/debug.sh",
    "test:gui:selenium": "./tests/gui/debug-selenium.sh",
    "_test:gui:fixtures": "cd playground && ./get-fixtures.js",
    "_test:gui": "TS_NODE_CACHE=0 mocha --opts tests/gui/mocha.opts",
    "_test:gui:debug": "TS_NODE_CACHE=0 SELENIUM_HOST=localhost mocha --opts tests/gui/mocha.debug.opts",
    "report-coverage": "nyc report --reporter=json && codecov -f tests/results/coverage/coverage-final.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx?": "eslint",
    "*.js": "eslint"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "dependencies": {
    "classnames": "latest",
    "reset.css": "~2.0.2",
    "store": "~2.0.12"
  },
  "peerDependencies": {
    "react": "^16",
    "react-cosmos": "^4"
  },
  "devDependencies": {
    "@babel/core": "~7.1.2",
    "@babel/plugin-proposal-class-properties": "~7.1.0",
    "@babel/preset-env": "~7.1.0",
    "@babel/preset-react": "~7.0.0",
    "@babel/preset-typescript": "~7.1.0",
    "@babel/register": "~7.0.0",
    "@nighttrax/eslint-config": "~1.0.2",
    "@types/chai": "~4.1.7",
    "@types/classnames": "~2.2.6",
    "@types/jquery": "~3.3.22",
    "@types/jsdom": "~12.2.0",
    "@types/mocha": "~5.2.5",
    "@types/node": "~10.12.2",
    "@types/react": "~16.4.18",
    "@types/react-dom": "~16.0.9",
    "@types/sinon": "~5.0.5",
    "@types/store": "~2.0.0",
    "@types/webdriverio": "~4.13.0",
    "babel-loader": "~8.0.4",
    "babel-plugin-istanbul": "~5.1.0",
    "babel-plugin-module-resolver": "~3.1.1",
    "chai": "~4.2.0",
    "codecov": "~3.1.0",
    "css-loader": "~1.0.1",
    "eslint": "~5.8.0",
    "eslint-import-resolver-babel-module": "~4.0.0",
    "eslint-plugin-import": "~2.14.0",
    "eslint-plugin-jsx-a11y": "~6.1.2",
    "eslint-plugin-react": "~7.11.1",
    "eslint-plugin-typescript": "~0.13.0",
    "html-webpack-plugin": "~3.2.0",
    "husky": "~1.1.3",
    "ignore-styles": "~5.0.1",
    "jquery": "~3.3.1",
    "jsdom": "~13.0.0",
    "less": "~3.8.1",
    "less-loader": "~4.1.0",
    "lint-staged": "~8.0.4",
    "mocha": "~5.2.0",
    "mugshot": "latest",
    "mugshot-looks-same": "~1.0.0",
    "mugshot-webdriverio": "latest",
    "nyc": "~13.1.0",
    "react": "~16.6.0",
    "react-cosmos": "~4.6.4",
    "react-cosmos-shared": "latest",
    "react-cosmos-voyager2": "latest",
    "react-dom": "~16.6.0",
    "react-mock-component": "~0.4.1",
    "sinon": "~7.1.1",
    "style-loader": "~0.23.1",
    "ts-node": "~7.0.1",
    "typemoq": "~2.1.0",
    "typescript": "~3.1.6",
    "typescript-eslint-parser": "~20.1.1",
    "webdriverio": "latest",
    "webpack": "~4.25.1"
  },
  "files": [
    "dist/src/**/*"
  ]
}
